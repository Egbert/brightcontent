<div id="overview">
  <h1><%= collection_name %></h1>

  <table>
    <thead>
      <tr>
        <% list_fields.each do |list_field| %>
          <th><%= list_field %></th>
        <% end %>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <%= render "list", collection: collection, list_fields: list_fields  %>
    </tbody>
  </table>
</div>
